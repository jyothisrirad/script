#!/bin/bash

s65=BungeeCord

s65() {
	oldmcvar=$mcver
	export mcver=$s65 
	
	mcpath=/mnt/runtimes/$mcver
	[ ! -d "$mcpath" ] && { echo -e "${RED}$mcpath not installed${NC}"; mcver=$oldmcvar; }
	[   -d "$mcpath" ] && { echo mcver=$mcver; cd $mcpath; }
}

alias 65=s65
alias bun=s65
alias ghub='65 && mcdo send all hub'
alias guhc='65 && mcdo send all uhc'
alias glist='65 && mcdo glist'
