#!/bin/bash

s77=spigot-1.11.2-bedwar-77

s77() {
	oldmcvar=$mcver
	export mcver=$s77 
	
	mcpath=/mnt/runtimes/$mcver
	[ ! -d "$mcpath" ] && { echo -e "${YELLOW}$mcpath not installed${NC}"; mcver=$oldmcvar; }
	[   -d "$mcpath" ] && { echo mcver=$mcver; cd $mcpath; }
}

alias v11b=s77
alias 77=s77
alias bed=s77
